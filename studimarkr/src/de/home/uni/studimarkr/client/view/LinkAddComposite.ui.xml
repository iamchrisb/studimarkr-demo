	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	>
<ui:with type="de.home.uni.studimarkr.client.ressources.MyRessources" field="res"></ui:with>
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
